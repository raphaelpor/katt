{
  "name": "katt",
  "version": "0.0.9",
  "description": "CLI tool that tests the output of agentic AI tools",
  "keywords": [
    "cli",
    "ai",
    "agentic-ai",
    "testing",
    "evaluation"
  ],
  "author": "Raphael Porto (https://github.com/raphaelpor)",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "katt": "dist/katt.js"
  },
  "scripts": {
    "build": "vite build",
    "dev": "tsx src/index.ts",
    "lint": "biome lint ./src",
    "format": "biome format --write ./src",
    "test": "vitest",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test:build": "node ./dist/katt.js",
    "test:build:codex": "node ./dist/katt.js --config-file=./katt-codex.json"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@biomejs/biome": "2.4.4",
    "@types/node": "25.3.0",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-dts": "4.5.4",
    "vitest": "4.0.18",
    "vscode-jsonrpc": "^8.2.1"
  },
  "dependencies": {
    "@github/copilot-sdk": "0.1.25"
  },
  "bugs": {
    "url": "https://github.com/raphaelpor/katt/issues"
  },
  "homepage": "https://github.com/raphaelpor/katt"
}
